    "def solution(wallet, bill): \n",
    "    answer = 0\n",
    "    while min(bill) > min(wallet) or max(bill) > max(wallet):\n",
    "        if bill[0] > bill[1]:\n",
    "            bill[0] /= 2\n",
    "        else:\n",
    "            bill[1] /=2\n",
    "        answer += 1\n",
    "    return answer\n",
    "\n",
    "print(solution([50, 50],[100, 241]))"
   ]
  }
